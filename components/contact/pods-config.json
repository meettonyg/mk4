{
  "dataSource": "pods",
  "description": "Contact component requires email, phone, skype, and address information. Website URLs are handled by Social component.",
  "fields": {
    "email": {
      "type": "email",
      "required": false,
      "description": "Contact email address"
    },
    "phone": {
      "type": "phone",
      "required": false,
      "description": "Contact phone number"
    },
    "skype": {
      "type": "text",
      "required": false,
      "description": "Skype username"
    },
    "address": {
      "type": "text",
      "required": false,
      "description": "Street address"
    },
    "city": {
      "type": "text",
      "required": false,
      "description": "City"
    },
    "state": {
      "type": "text",
      "required": false,
      "description": "State/Province"
    },
    "zip": {
      "type": "text",
      "required": false,
      "description": "Zip/Postal code"
    },
    "country": {
      "type": "text",
      "required": false,
      "description": "Country"
    }
  }
}
